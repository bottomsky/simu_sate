@PACKAGE_INIT@

include(CMakeFindDependencyMacro)

# 查找依赖项
find_dependency(Eigen3 3.3 REQUIRED)

# 包含目标文件
include("${CMAKE_CURRENT_LIST_DIR}/J2OrbitPropagatorTargets.cmake")

# 检查组件
check_required_components(J2OrbitPropagator)

# 设置变量
set(J2OrbitPropagator_FOUND TRUE)
set(J2OrbitPropagator_VERSION @PROJECT_VERSION@)
set(J2OrbitPropagator_INCLUDE_DIRS "@PACKAGE_CMAKE_INSTALL_INCLUDEDIR@")
set(J2OrbitPropagator_LIBRARIES J2OrbitPropagator::j2_orbit_propagator_shared)
set(J2OrbitPropagator_STATIC_LIBRARIES J2OrbitPropagator::j2_orbit_propagator_static)

# 向后兼容
set(J2ORBITPROPAGATOR_FOUND ${J2OrbitPropagator_FOUND})
set(J2ORBITPROPAGATOR_INCLUDE_DIRS ${J2OrbitPropagator_INCLUDE_DIRS})
set(J2ORBITPROPAGATOR_LIBRARIES ${J2OrbitPropagator_LIBRARIES})

message(STATUS "Found J2OrbitPropagator: ${J2OrbitPropagator_VERSION}")